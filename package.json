{
  "name": "@radoman93/ticketseat-io-angular",
  "version": "1.1.1",
  "description": "Angular library for ticket seat management with drag-and-drop functionality",
  "main": "dist/bundles/ticketseat-io-angular.umd.js",
  "module": "dist/fesm2015/ticketseat-io-angular.js",
  "es2015": "dist/fesm2015/ticketseat-io-angular.js",
  "esm5": "dist/esm5/ticketseat-io-angular.js",
  "esm2015": "dist/esm2015/ticketseat-io-angular.js",
  "fesm5": "dist/fesm5/ticketseat-io-angular.js",
  "fesm2015": "dist/fesm2015/ticketseat-io-angular.js",
  "typings": "dist/ticketseat-io-angular.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib && npm run add:css-export && npm run build:css && npm run copy:css",
    "build:lib": "ng-packagr -p ng-package.json",
    "add:css-export": "node add-css-export.js",
    "build:css": "npx tailwindcss -i ./src/lib-styles.css -o ./temp-styles.css --minify",
    "copy:css": "cp ./temp-styles.css ./dist/styles.css && rm ./temp-styles.css",
    "prebuild": "echo 'Building library...'",
    "postbuild": "npm run verify",
    "verify": "node verify-package.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack": "npm pack dist",
    "publish:github": "cd dist && npm publish --registry=https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radoman93/ticketseat-io-angular.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "angular",
    "tickets",
    "seating",
    "drag-drop",
    "event-management",
    "mobx"
  ],
  "author": "Pavle Radoman",
  "license": "MIT",
  "peerDependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "mobx": "^6.0.0",
    "mobx-angular": "^4.0.0"
  },
  "dependencies": {
    "daisyui": "^4.12.24",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.12",
    "@angular/animations": "^19.2.11",
    "@angular/cli": "^19.2.12",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mobx": "^6.13.7",
    "mobx-angular": "^4.8.0",
    "ng-packagr": "^19.0.0",
    "typescript": "~5.7.2"
  }
}
